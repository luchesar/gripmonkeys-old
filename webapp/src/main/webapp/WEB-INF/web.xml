<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>'Curso de conducir' web app</display-name>

	<servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>org.bitbucket.cursodeconducir.services.fileupload.Upload</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>BlobServe</servlet-name>
        <servlet-class>org.bitbucket.cursodeconducir.services.fileupload.BlobServe</servlet-class>
    </servlet>
    
	<servlet-mapping>
		<servlet-name>Upload</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>BlobServe</servlet-name>
		<url-pattern>/blob</url-pattern>
	</servlet-mapping>
</web-app>